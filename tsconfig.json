{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@nuxtjs/i18n", "@pinia/nuxt"]
  },
  "include": ["app/**/*", "server/**/*"],
  "exclude": [
    "node_modules",
    "app/pages/join.vue"
  ]
}
